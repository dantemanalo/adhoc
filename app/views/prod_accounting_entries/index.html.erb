<p id="notice"><%= notice %></p>

<h1>Prod Accounting Entries</h1>

<table class="table" border = 1>
  <thead>
    <tr>
      <th>Assured No.</th>
      <th>Assured Name</th>
    </tr>
  </thead>

  <tbody>
    <% @prod_accounting_entries.each do |prod_accounting_entry| %>
      <tr>
        <td><%= prod_accounting_entry.assd_num %></td>
        <td><%= prod_accounting_entry.assd_nam %></td>
        <td><%= link_to 'Show', prod_accounting_entry %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prod Accounting Entry', new_prod_accounting_entry_path %>
